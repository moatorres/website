{
  "name": "blog",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/blog",
  "projectType": "application",
  "tags": [],
  "targets": {
    "bootstrap": {
      "executor": "nx:run-commands",
      "inputs": [
        "{projectRoot}/src/content/**/*",
        "{projectRoot}/src/utils/config.ts"
      ],
      "outputs": ["{projectRoot}/src/data"],
      "cache": true,
      "options": {
        "env": {
          "NODE_OPTIONS": "--no-warnings --loader @mdx-js/node-loader"
        },
        "cwd": "{workspaceRoot}",
        "command": "tsx apps/blog/src/utils/bootstrap.ts {projectRoot}"
      }
    },
    "build": {
      "dependsOn": ["bootstrap"]
    },
    "dev": {
      "dependsOn": ["bootstrap"]
    }
  }
}
